{
  "manifest_version": 2,
  "name": "Auto Send Message",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": ["https://www.chatwork.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Auto send message popup"
  }
}